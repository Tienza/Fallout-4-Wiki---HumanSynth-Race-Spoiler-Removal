{
  "manifest_version": 2,

  "name": "Fallout Race Removal",
  "description": "This extension hides spoilers pertaining to character race",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
